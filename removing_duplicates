#include<iostream>
using namespace std;

main()
{
    int a[20];

   cout<<"Enter array size: "<<endl;
    int n;
    cin>>n;

    cout<<"Enter array element: "<<endl;
    for(int i = 0; i < n; i++)
    {
        cin>>a[i];
    }

    cout<<"Array before removing duplicates : "<<endl;
    for(int i = 0; i < n; i++)
    {
        cout<<a[i]<<" ";
    }
    cout<<endl;
   cout<<"Array before removing duplicates :"<<endl;
    for(int i = 0; i < n; i++)
    {
        for(int j = i+1; j < n; )
        {
            if(a[j] == a[i])
            {
                for(int k = j; k < n; k++)
                {
                    a[k] = a[k+1];
                }
                n--;
            }
            else
            {
               j++;
            }
        }
    }

    for(int i = 0; i < n; i++)
    {
         cout<<a[i]<<" ";
    }

}
